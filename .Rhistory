colormap = function(lowcol, highcol, ncols = 1000){
cfun = grDevices::colorRampPalette(c(lowcol, highcol))(ncols)
f = function(vals){
normvals = round(1000*(vals - min(vals))/diff(range(vals)))
return(cfun(vals))
}
}
f = colormap(c('red', 'blue'))
f = colormap('red', 'blue')
plot(1:10, 1:10, col = f(1:10))
